{
    "version": 2,
    "target_type": "raspberrypi-bullseye",
    "boards": [
      "RSPi3B",
      "RSPiZero",
      "RSPiCM3",
      "RSPiZeroW",
      "RSPi3BPlus",
      "RSPi3APlus",
      "RSPiCM3Plus",
      "RSPiZero2W",
      "RSPi4B"
    ],
    "devices": [
      {
        "type": "camera"
      },
      {
        "type": "videocore",
        "device": [
          "/dev/vcsm",
          "/dev/vcio",
          "/dev/vc-mem",
          "/dev/vchiq",
          "/dev/gpiomem",
          "/dev/dri/card0"
        ]
      },
      {
        "type": "framebuffer",
        "device": [
          "/dev/fb0"
        ],
        "required": false
      }
    ],
    "networks": [
      {
        "domain": "kinesisvideo.*.amazonaws.com",
        "port": 443,
        "protocol": "tcp"
      },
      {
        "domain": "*.kinesisvideo.*.amazonaws.com",
        "port": 443,
        "protocol": "tcp"
      }
    ]
  }
  