{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "array",
    "items": {
        "type": "object",
        "properties": {
            "prob": {
                "title": "probability",
                "description": "matching score",
                "descriptions": {
                    "ja": "確度"
                },
                "type": "number",
                "minimum": 0.0,
